#include <stdio.h>
int input (int *a, int *b)
{
   printf ("Enter a number \n");
   scanf ("%d", a);
   printf ("Enter a number \n");
   scanf ("%d", b);
}
int compute (int a, int b)
{
  int  x=a+b;
   return x;
}
void output (int m, int n, int o)
{
   printf ("%d+%d is %d", m,n,o);
}
int main (void)
{
   int a,b;
   input(&a,&b);
   int c=compute(a,b);
   output (a,b,c);
}
   
   
   
